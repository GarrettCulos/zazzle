{
	"rules": {
		"class-name": true,
		"no-var-keyword": true,
		"whitespace": [
			true,
			"check-branch",
			"check-decl",
			"check-operator",
			"check-module",
			"check-separator",
			"check-type"
		],
		"no-internal-module": true,
		"no-trailing-whitespace": false,
		"no-null-keyword": true,
		"prefer-const": true,
		"jsdoc-format": true
	}
}